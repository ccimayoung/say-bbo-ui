{
  "name": "say-bbo-ui",
  "version": "1.0.10",
  "description": "A modern React UI component library with CSS-in-JS",
  "main": "dist/index.cjs.js",
  "files": [
    "dist"
  ],
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:components": "tsc && vite build --config vite.config.components.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "author": "ayoung.shim",
  "license": "MIT",
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "storybook": "^8.6.14",
    "typescript": "5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.3.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "^7.4.1",
    "react-router-dom": "^7.4.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./Button": {
      "import": "./dist/components/Button/index.esm.js",
      "require": "./dist/components/Button/index.cjs.js",
      "types": "./dist/components/Button/index.d.ts"
    },
    "./Modal": {
      "import": "./dist/components/Modal/index.esm.js",
      "require": "./dist/components/Modal/index.cjs.js",
      "types": "./dist/components/Modal/index.d.ts"
    }
  }
}
