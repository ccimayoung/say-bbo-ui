{
  "name": "say-bbo-ui",
  "version": "1.0.23",
  "description": "A modern React UI component library with CSS-in-JS",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:components": "tsc && vite build --config vite.config.components.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --config-dir .storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "author": "ayoung.shim",
  "license": "MIT",
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.6.14",
    "typescript": "5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.3.0",
    "gh-pages": "^6.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./Button": {
      "types": "./dist/components/Button/index.d.ts",
      "import": "./dist/components/Button/index.esm.js",
      "require": "./dist/components/Button/index.cjs.js"
    },
    "./Modal": {
      "types": "./dist/components/Modal/index.d.ts",
      "import": "./dist/components/Modal/index.esm.js",
      "require": "./dist/components/Modal/index.cjs.js"
    },
    "./Icon": {
      "types": "./dist/components/Icon/index.d.ts",
      "import": "./dist/components/Icon/index.esm.js",
      "require": "./dist/components/Icon/index.cjs.js"
    },
    "./Select": {
      "types": "./dist/components/Select/index.d.ts",
      "import": "./dist/components/Select/index.esm.js",
      "require": "./dist/components/Select/index.cjs.js"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "import": "./dist/styles.esm.js",
      "require": "./dist/styles.cjs.js"
    }
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "Button": [
        "./dist/components/Button/index.d.ts"
      ],
      "Modal": [
        "./dist/components/Modal/index.d.ts"
      ],
      "Icon": [
        "./dist/components/Icon/index.d.ts"
      ],
      "Select": [
        "./dist/components/Select/index.d.ts"
      ],
      "styles": [
        "./dist/styles.d.ts"
      ]
    }
  }
}
